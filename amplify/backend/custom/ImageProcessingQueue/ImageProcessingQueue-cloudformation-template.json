{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "env": {
      "Type": "String"
    }
  },
  "Resources": {
    "ImageProcessingQueue": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "QueueName": {
          "Fn::Join": [
            "-",
            [
              "ImageProcessingQueue",
              {
                "Ref": "env"
              }
            ]
          ]
        },
        "VisibilityTimeout": 3600
      }
    }
  },
  "Outputs": {
    "Url": {
      "Description": "Image Processing Queue URL",
      "Value": {
        "Ref": "ImageProcessingQueue"
      }
    },
    "Arn": {
      "Description": "Image Processing Queue ARN",
      "Value": {
        "Fn::GetAtt": [
          "ImageProcessingQueue",
          "Arn"
        ]
      }
    },
    "Name": {
      "Description": "Image Processing Queue Name",
      "Value": {
        "Fn::GetAtt": [
          "ImageProcessingQueue",
          "QueueName"
        ]
      }
    }
  },
  "Description": "{\"createdOn\":\"Mac\",\"createdBy\":\"Amplify\",\"createdWith\":\"7.6.4\",\"stackType\":\"custom-customCloudformation\",\"metadata\":{}}"
}